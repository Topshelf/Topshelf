<?xml version="1.0"?>
<configuration>
	<startup>
		<supportedRuntime version="v2.0.50727"/>
	</startup>
	<runtime>
		<legacyUnhandledExceptionPolicy enabled="1" />
	</runtime>
	<appSettings>
		<!-- use either None, 	FailToCreate,		FailToStart,		FailToStop,		CrashAfterStart -->
		<add key="failureType" value="CrashAfterStart"/>
	</appSettings>
</configuration>
