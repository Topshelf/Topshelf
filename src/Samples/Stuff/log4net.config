<?xml version="1.0" encoding="utf-8" ?>
<log4net>
  <appender name="main" type="log4net.Appender.ConsoleAppender">
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%-5level - %message%newline" />
    </layout>
  </appender>

  <root>
    <level value="DEBUG" />
    <appender-ref ref="main" />
  </root>
</log4net>